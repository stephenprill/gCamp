

<div class = "container">
  <div class = "page-header">
    <h1>Users</h1>
      <div class = "pull-right">
        <%= link_to "Create User", new_user_path, :class => "btn btn-primary"%>
      </div>
    </div>


<div class="container">
  <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>E-mail</th>
          <th colspan="3">
          </th>
        </tr>
      </thead>



      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td class="col-md-2"><%= user.first_name %><%= user.last_name %></td>
          <td width="col-md-2"><%= user.email %></td>
          <td width="30px"><%= link_to "Edit", edit_user_path(user) %></td>

        </tr>
        <% end %>

      </tbody>



    </table>
</div>

  </div>
</div>
