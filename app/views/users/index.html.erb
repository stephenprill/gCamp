

<div class = "container">
  <div class = "page-header">
    <h1>Users</h1>
      <div class = "pull-right">
        <%= link_to "Create User", new_user_path, :class => "btn btn-primary"%>
      </div>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>E-mail</th>
        </tr>
      </thead>

    

      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
          <td><%= link_to "Edit", edit_user_path(user) %></td>
        </tr>
        <% end %>

      </tbody>



    </table>

  </div>
</div>
